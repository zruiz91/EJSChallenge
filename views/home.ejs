<%- include('partials/header') -%>

    <h1>Home</h1>
    <p>
        <%= startingContent %>
    </p>
    <p>
        <% posts.forEach(function(post){ %>

            <h1>
                <%= post.title%>
            </h1>
            <p>
                <%=  post.content.substring(0,30) + " ..." %>
                <a href="/posts/ <%=post.title %>">Read More</a>
            </p>

            <% }) %>
    </p>

    <%- include('partials/footer') -%>